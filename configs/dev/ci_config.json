{
    "dataset_mixture": {
        "datasets": [
            {
                "grounding": "dummy"
            },
            {
                "repo_id": "lerobot/droid_100",
                "episodes": [50, 10]
            }
        ],
        "weights": [
            1.0,
            1.0
        ],
        "action_freq": 30.0,
        "image_resample_strategy": "nearest",
        "vector_resample_strategy": "nearest"
    },
    "policy": {
        "type": "pi05",
        "pretrained_path": "TensorAuto/pi05_base",
        "n_obs_steps": 1,
        "normalization_mapping": {
            "VISUAL": "IDENTITY",
            "STATE": "MIN_MAX",
            "ACTION": "MEAN_STD"
        },
        "chunk_size": 10,
        "predict_response": true,
        "n_action_steps": 10,
        "max_state_dim": 32,
        "max_action_dim": 32,
        "proj_width": 1024,
        "num_steps": 10,
        "init_strategy": "expert_only_he_init",
        "attention_implementation": "eager",
        "freeze_vision_encoder": true,
        "train_expert_only": true,
        "prompt_max_length": 52,
        "response_max_length": 5,
        "discrete_action_max_length": 10
    },
    "resume": false,
    "seed": 1000,
    "resolution": [224, 224],
    "num_cams": 2,
    "max_state_dim": 32,
    "max_action_dim": 32,
    "action_chunk": 10,
    "loss_weighting": {"MSE": 1, "CE": 1},
    "num_workers": 12,
    "batch_size": 4,
    "gradient_accumulation_steps": 2,
    "dataloader_batch_size": 2,
    "prefetch_factor": 8,
    "steps": 25,
    "log_freq": 1,
    "save_checkpoint": true,
    "save_freq": 1000,
    "use_policy_training_preset": true,
    "debug": true,
    "optimizer": {
        "type": "adamw",
        "lr": 1e-4,
        "weight_decay": 0,
        "grad_clip_norm": 10.0,
        "betas": [
            0.9,
            0.95
        ],
        "eps": 1e-08
    },
    "scheduler": {
        "type": "cosine_decay_with_warmup",
        "num_warmup_steps": 0,
        "num_decay_steps": 30000,
        "peak_lr": 1e-4,
        "decay_lr": 0
    },
    "wandb": {
        "enable": true,
        "entity": "wyautox-autox",
        "project": "github-ci",
        "run_id": null,
        "name": null,
        "notes": "GitHub CI Config",
        "tags": [],
        "group": null,
        "job_type": null,
        "mode": null,
        "allow_resume": true,
        "disable_artifact": false
    }
}
